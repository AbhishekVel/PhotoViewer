<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="615.0" stylesheets="@default.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller">
   <children>
      <BorderPane>
         <left>
            <Accordion BorderPane.alignment="CENTER">
              <panes>
                <TitledPane animated="false" minWidth="-Infinity" text="untitled 1" />
                <TitledPane animated="false" text="untitled 2" />
              </panes>
            </Accordion>
         </left>
         <top>
            <TextField fx:id="textfield" alignment="TOP_CENTER" editable="false" prefHeight="31.0" prefWidth="627.0" text="Title" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <ImageView fx:id="image" fitHeight="315.0" fitWidth="488.0" pickOnBounds="true" />
         </center>
      </BorderPane>
      <HBox alignment="BOTTOM_CENTER" layoutX="20.0" layoutY="346.0" prefHeight="48.0" prefWidth="588.0">
         <children>
            <Button fx:id="load" mnemonicParsing="false" prefHeight="46.0" prefWidth="65.0" text="Load">
               <HBox.margin>
                  <Insets right="200.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="back" mnemonicParsing="false" text="&lt;-" textFill="#e80505">
               <font>
                  <Font size="17.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Button>
            <Button fx:id="pause" mnemonicParsing="false" text="|| ">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Button fx:id="forward" mnemonicParsing="false" text="-&gt;" textFill="#e80505">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets right="200.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
